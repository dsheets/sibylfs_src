Tau
Pid 2 <- -

Pid 2 -> open /tmp_dir_1490/f1.txt [O_CREAT;O_RDWR] <---rwx--->
Tau
Pid 2 <- -

Pid 3 -> open /tmp_dir_1490/f1.txt [O_RDONLY]
Tau
Pid 3 <- -

dump-internal

